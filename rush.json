{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.62.4",
  "pnpmVersion": "7.33.6",
  "pnpmOptions": {
    "useWorkspaces": true,
    "strictPeerDependencies": true
  },
  "nodeSupportedVersionRange": ">=16",
  "gitPolicy": {},

  "repository": {
    "url": "https://github.com/dills122/session-chat",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [
      // "common/scripts/pre-rush-install.js"
    ],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [],
  "projects": [
    {
      "packageName": "chat-backend",
      "projectFolder": "apps/chat-backend"
    },
    {
      "packageName": "chat-frontend",
      "projectFolder": "apps/chat-frontend"
    }
  ]
}
